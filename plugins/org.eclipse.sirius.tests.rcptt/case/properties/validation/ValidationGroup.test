with [get-view Properties | get-section Validation] {
    with [get-editbox -index 1] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Text info message..." | verify-true
    }
    with [get-editbox -index 3] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Text warning message..." | verify-true
    }
    with [get-editbox -index 5] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Text error message..." | verify-true
    }
    with [get-editbox -index 7] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Text info/warning/error message..." | verify-true
    }
    with [get-editbox -index 9] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Text info message..." | verify-true
    }
    with [get-button Label] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Button error message..." | verify-true
    }
    with [get-editbox -index 12] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Label info message..." | verify-true
    }
    with [get-button "Checkbox Error"] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Checkbox error message..." | verify-true
    }
    with [get-combo] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Select error message..." | verify-true
    }
    with [get-editbox -index 16] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Text area error message..." | verify-true
    }
    with [get-control Any -index 36] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Radio error message..." | verify-true
    }
    with [get-label Element1] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Reference error message..." | verify-true
    }
    with [get-editbox -after [get-label Element1] -index 2] {
