        get-property "decorators[0].description" | equals "Hyperlink error message..." | verify-true
    }
    with [get-editbox -after [get-label Element1] -index 4] {
        get-property "decorators[0].visible" | equals true | verify-true
        get-property "decorators[0].description" | equals "Dynamic mapping error message..." | verify-true
    }
}
